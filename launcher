rem @echo off
title Minecraft Launcher

:: set username
set player=USERNAMEE

:: main directory of the game
set MC_DIR=%APPDATA%\.minecraft

:: Are stored here...
:: saves/, resourcepacks/ and screenshots/ dirs
:: and options.txt and servers.dat files
set GAME_DIR=%MC_DIR%

:: libraries and resource dir (requires no changes)
set ASSETS_DIR=%MC_DIR%\assets
set ASSETS_INDEX=1.17

:: version minecraft (from versions/ dir)
set GAME_VERSION=1.17.1

:: get archive from natives/ dir
set NATIVES_DIR=%MC_DIR%\bin\bebrasmp%GAME_VERSION%

:: *.jar paths generated by make_libs_list.py
set LIBRARIES_LIST=%MC_DIR%/libraries/com/mojang/blocklist/1.0.5/blocklist-1.0.5.jar;%MC_DIR%/libraries/com/mojang/patchy/2.1.6/patchy-2.1.6.jar;%MC_DIR%/libraries/com/github/oshi/oshi-core/5.7.5/oshi-core-5.7.5.jar;%MC_DIR%/libraries/net/java/dev/jna/jna/5.8.0/jna-5.8.0.jar;%MC_DIR%/libraries/net/java/dev/jna/jna-platform/5.8.0/jna-platform-5.8.0.jar;%MC_DIR%/libraries/org/slf4j/slf4j-api/1.8.0-beta4/slf4j-api-1.8.0-beta4.jar;%MC_DIR%/libraries/org/apache/logging/log4j/log4j-slf4j18-impl/2.14.1/log4j-slf4j18-impl-2.14.1.jar;%MC_DIR%/libraries/com/ibm/icu/icu4j/66.1/icu4j-66.1.jar;%MC_DIR%/libraries/com/mojang/javabridge/1.1.23/javabridge-1.1.23.jar;%MC_DIR%/libraries/net/sf/jopt-simple/jopt-simple/5.0.3/jopt-simple-5.0.3.jar;%MC_DIR%/libraries/io/netty/netty-all/4.1.25.Final/netty-all-4.1.25.Final.jar;%MC_DIR%/libraries/com/google/guava/guava/21.0/guava-21.0.jar;%MC_DIR%/libraries/org/apache/commons/commons-lang3/3.5/commons-lang3-3.5.jar;%MC_DIR%/libraries/commons-io/commons-io/2.5/commons-io-2.5.jar;%MC_DIR%/libraries/commons-codec/commons-codec/1.10/commons-codec-1.10.jar;%MC_DIR%/libraries/net/java/jinput/jinput/2.0.5/jinput-2.0.5.jar;%MC_DIR%/libraries/net/java/jutils/jutils/1.0.0/jutils-1.0.0.jar;%MC_DIR%/libraries/com/mojang/brigadier/1.0.18/brigadier-1.0.18.jar;%MC_DIR%/libraries/com/mojang/datafixerupper/4.0.26/datafixerupper-4.0.26.jar;%MC_DIR%/libraries/com/google/code/gson/gson/2.8.0/gson-2.8.0.jar;%MC_DIR%/libraries/com/mojang/authlib/2.3.31/authlib-2.3.31.jar;%MC_DIR%/libraries/org/apache/commons/commons-compress/1.8.1/commons-compress-1.8.1.jar;%MC_DIR%/libraries/org/apache/httpcomponents/httpclient/4.3.3/httpclient-4.3.3.jar;%MC_DIR%/libraries/commons-logging/commons-logging/1.1.3/commons-logging-1.1.3.jar;%MC_DIR%/libraries/org/apache/httpcomponents/httpcore/4.3.2/httpcore-4.3.2.jar;%MC_DIR%/libraries/it/unimi/dsi/fastutil/8.2.1/fastutil-8.2.1.jar;%MC_DIR%/libraries/org/apache/logging/log4j/log4j-api/2.14.1/log4j-api-2.14.1.jar;%MC_DIR%/libraries/org/apache/logging/log4j/log4j-core/2.14.1/log4j-core-2.14.1.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl/3.2.1/lwjgl-3.2.1.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl/3.2.2/lwjgl-3.2.2.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-jemalloc/3.2.1/lwjgl-jemalloc-3.2.1.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-jemalloc/3.2.2/lwjgl-jemalloc-3.2.2.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-openal/3.2.1/lwjgl-openal-3.2.1.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-openal/3.2.2/lwjgl-openal-3.2.2.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-opengl/3.2.1/lwjgl-opengl-3.2.1.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-opengl/3.2.2/lwjgl-opengl-3.2.2.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-glfw/3.2.1/lwjgl-glfw-3.2.1.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-glfw/3.2.2/lwjgl-glfw-3.2.2.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-stb/3.2.1/lwjgl-stb-3.2.1.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-stb/3.2.2/lwjgl-stb-3.2.2.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-tinyfd/3.2.1/lwjgl-tinyfd-3.2.1.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-tinyfd/3.2.2/lwjgl-tinyfd-3.2.2.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl/3.2.1/lwjgl-3.2.1.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl/3.2.2/lwjgl-3.2.2.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-jemalloc/3.2.1/lwjgl-jemalloc-3.2.1.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-jemalloc/3.2.2/lwjgl-jemalloc-3.2.2.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-openal/3.2.1/lwjgl-openal-3.2.1.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-openal/3.2.2/lwjgl-openal-3.2.2.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-opengl/3.2.1/lwjgl-opengl-3.2.1.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-opengl/3.2.2/lwjgl-opengl-3.2.2.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-glfw/3.2.1/lwjgl-glfw-3.2.1.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-glfw/3.2.2/lwjgl-glfw-3.2.2.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-stb/3.2.1/lwjgl-stb-3.2.1.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-tinyfd/3.2.2/lwjgl-tinyfd-3.2.2.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-tinyfd/3.2.1/lwjgl-tinyfd-3.2.1.jar;%MC_DIR%/libraries/org/lwjgl/lwjgl-stb/3.2.2/lwjgl-stb-3.2.2.jar;%MC_DIR%/libraries/com/mojang/text2speech/1.11.3/text2speech-1.11.3.jar;%MC_DIR%/libraries/com/mojang/text2speech/1.11.3/text2speech-1.11.3.jar;%MC_DIR%/libraries/ca/weblite/java-objc-bridge/1.0.0/java-objc-bridge-1.0.0.jar;%MC_DIR%/libraries/ca/weblite/java-objc-bridge/1.0.0/java-objc-bridge-1.0.0.jar;%MC_DIR%/versions/%GAME_VERSION%/%GAME_VERSION%.jar

:: player nickname (the over head)
set PLAYER_NAME=%player%

:: game window resolution
set WINDOW_W=800
set WINDOW_H=640

:: java machine arguments
set "JVM_RAM=-Xmx2G"
set "JVM_ARGS=-XX:+UnlockExperimentalVMOptions -XX:+UseG1GC -XX:G1NewSizePercent=20 -XX:G1ReservePercent=20 -XX:MaxGCPauseMillis=50 -XX:G1HeapRegionSize=32M"

:: running minecraft
%SystemDrive%
cd %MC_DIR%

java.exe %JVM_RAM% %JVM_ARGS% ^
-Djava.library.path=%NATIVES_DIR% ^
-cp %LIBRARIES_LIST% ^
net.minecraft.client.main.Main ^
--username %PLAYER_NAME% ^
--version %GAME_VERSION% ^
--accessToken 0 --userProperties {} ^
--gameDir %GAME_DIR% ^
--assetsDir %ASSETS_DIR% ^
--assetIndex %ASSETS_INDEX% ^
--width %WINDOW_W% ^
--height %WINDOW_H%
